{
  "name": "devpulse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:ui": "node scripts/test-desktop-ui.js",
    "test:ui:restore": "node -e \"require('fs').copyFileSync('src/app/dashboard/page.tsx.backup', 'src/app/dashboard/page.tsx')\"",
    "prepare:dry-run": "node scripts/prepare-dry-run.js",
    "dry-run": "npm run prepare:dry-run && npm run dev",
    "test:e2e": "playwright test --config=e2e-tests/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=e2e-tests/playwright.config.ts --ui",
    "test:e2e:debug": "playwright test --config=e2e-tests/playwright.config.ts --debug",
    "test:e2e:ci": "playwright test --config=e2e-tests/playwright.config.ts --reporter=github",
    "fix:tailwind": "node scripts/fix-tailwind.js",
    "reset:tailwind": "node scripts/reset-tailwind.js",
    "verify:tailwind": "node scripts/verify-tailwind.js",
    "css:check": "next dev -p 3001 && open http://localhost:3001/dev/css-diagnostic",
    "tailwind:test": "node scripts/start-tailwind-test.js",
    "audit:security": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf .next node_modules/.cache",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "ci:setup": "npm ci && npm run db:generate && npm run db:push",
    "ci:test": "npm run lint && npm run type-check && npm run test:ci",
    "ci:build:full": "npm run db:generate && npm run build",
    "ci:e2e": "npm run prepare:dry-run && npm run test:e2e:ci",
    "setup:ci-cd": "node scripts/setup-ci-cd.js",
    "setup:external-services": "node scripts/setup-external-services.js",
    "verify:external-services": "node scripts/verify-external-services.js",
    "verify:ci-cd": "node scripts/verify-ci-operational.js",
    "validate:ci-cd": "node scripts/validate-ci-setup.js",
    "vercel-build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "build:vercel": "SKIP_ENV_VALIDATION=1 SKIP_TYPE_CHECK=true npm run vercel-build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@faker-js/faker": "^9.9.0",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-tabs": "^1.0.4",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.19.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "bullmq": "^5.1.1",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.525.0",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "next-themes": "^0.4.6",
    "openai": "^5.10.1",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^18",
    "react-responsive": "^10.0.1",
    "recharts": "^2.12.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.5.6",
    "autoprefixer": "^10.4.21",
    "@tailwindcss/container-queries": "^0.1.1",
    "typescript": "^5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "better-npm-audit": "^3.7.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-check-updates": "^16.14.12",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2"
  }
}
